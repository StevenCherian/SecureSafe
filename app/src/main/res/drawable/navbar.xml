<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="#16222B" />
        </shape>
    </item>
    <item android:top="1dp" android:left="-3dp" android:right="-3dp" android:bottom="-3dp" android:gravity="top">
        <shape
            android:shape="rectangle">
            <stroke android:width="1dp" android:color="#20313E" />
            <solid android:color="#00000000" />
        </shape>
    </item>
</layer-list>